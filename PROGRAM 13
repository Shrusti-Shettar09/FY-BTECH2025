#include <iostream>
#include<iomanip>
using namespace std;
int main() {
    int num,i=1;
    cout<<"enter a number"<<endl;
    cin>>num;
    do{
        cout<<num<<" * "<<i<<" = "<<num*i<<endl;
        i++;
       } while(i<=10);
        return 0;
}


output
enter a number
4
4 * 1 = 4
4 * 2 = 8
4 * 3 = 12
4 * 4 = 16
4 * 5 = 20
4 * 6 = 24
4 * 7 = 28
4 * 8 = 32
4 * 9 = 36
4 * 10 = 40


