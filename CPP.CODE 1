#include <iostream>
using namespace std;

int main() {
    float percentage;

    cout << "Enter your percentage: ";
    cin >> percentage;

    if (percentage >= 90) {
        cout << "Grade: A+" << endl;
    } 
    else if (percentage >= 80) {
        cout << "Grade: A" << endl;
    } 
    else if (percentage >= 70) {
        cout << "Grade: B" << endl;
    } 
    else if (percentage >= 60) {
        cout << "Grade: C" << endl;
    } 
    else if (percentage >= 50) {
        cout << "Grade: D" << endl;
    } 
    else if (percentage >= 40) {
        cout << "Grade: E" << endl;
    } 
    else {
        cout << "Grade: Fail" << endl;
    }

return 0;
}


OUTPUT
Enter your percentage: 90
Grade: A+


=== Code Execution Successful ===
