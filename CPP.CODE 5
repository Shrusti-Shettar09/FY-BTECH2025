#include <iostream>
using namespace std;
class Number {
public:
Number() {
int n;
cout << "Enter a number: ";
cin >> n;
cout << "Square: " << n*n << "\nCube: " << n*n*n << endl;
}
};
int main() {
Number obj;
return 0;
}


OUTPUT
Enter a number: 6
Square: 36
Cube: 216


