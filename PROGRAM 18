  #include<iostream>
  #include<iomanip>
  using namespace std;
  
  void swap(int a,int b){
      int t=a;
      a=b;
      b=t;
      cout<<"a after swap"<<a<<endl;
      cout<<"b after swap"<<b<<endl;
  }
  int main(){
      int x;
      int y;
      cout<<"enter a and b"<<endl;
      cin>>x;
      cin>>y;
      cout<<"a before swap"<< x <<endl;
      cout<<"b before swap"<< y <<endl;
      swap(x,y);
  
  return 0;
  }


output
enter a and b
5 6
a before swap5
b before swap6
a after swap6
b after swap5


=== Code Execution Successful ===
